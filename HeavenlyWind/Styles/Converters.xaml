<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:rbc="clr-namespace:Sakuno.Converters;assembly=Sakuno.Base"
                    xmlns:ruic="clr-namespace:Sakuno.UserInterface.Converters;assembly=Sakuno.UserInterface">

    <rbc:InvertBooleanConverter x:Key="InvertBooleanConverter" />
    <rbc:EqualityToBooleanConverter x:Key="EqualityToBooleanConverter" />

    <rbc:IsGreaterThanConverter x:Key="IsGreaterThanConverter" />
    <rbc:IsGreaterThanOrEqualToConverter x:Key="IsGreaterThanOrEqualToConverter" />
    <rbc:IsLessThanConverter x:Key="IsLessThanConverter" />
    <rbc:IsLessThanOrEqualToConverter x:Key="IsLessThanOrEqualToConverter" />

    <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
    <ruic:BooleanToInvisibilityConverter x:Key="BooleanToInvisibilityConverter" />

    <rbc:OppositeNumberConverter x:Key="OppositeNumberConverter" />

    <ruic:DockToOrientationConverter x:Key="DockToOrientationConverter" />

</ResourceDictionary>