<Project Sdk="Microsoft.NET.Sdk">

  <Import Project="$(SolutionDir)src\Sakuno.ING.props" />

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <PlatformTarget>x64</PlatformTarget>
    <RootNamespace>Sakuno.ING</RootNamespace>
    <AssemblyName>IntelligentNavalGun</AssemblyName>
    <Version>0.1.0-blueprint</Version>
    <Description>The canonical launcher of Intelligent Naval Gun</Description>
    <AutoGenerateBindingRedirect>true</AutoGenerateBindingRedirect>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Sakuno.ING.Bootstrap" Version="0.1.0-blueprint3" />
    <PackageReference Include="Sakuno.ING.Core.CommonSettings" Version="0.1.0-blueprint" />
    <PackageReference Include="Sakuno.ING.Core.DateTime" Version="0.1.0-blueprint" />
    <PackageReference Include="Sakuno.ING.Game.Models" Version="0.1.0-blueprint" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Sakuno.ING.Core.Data.Desktop\Sakuno.ING.Core.Data.Desktop.csproj" />
    <ProjectReference Include="..\Sakuno.ING.Core.Shell.Desktop\Sakuno.ING.Core.Shell.Desktop.csproj" />
  </ItemGroup>

</Project>
